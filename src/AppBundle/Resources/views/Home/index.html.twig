{% endblock %}

{% block body %}


    <div class="bg">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 mx-auto">
                    <h1>Eglės Nagų Studija</h1>
                    <a href="#" class="btn btn-lg btn-outline js-scroll-trigger">
